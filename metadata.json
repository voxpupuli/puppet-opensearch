{
  "name": "puppet-opensearch",
  "version": "1.1.1-rc0",
  "author": "Vox Pupuli",
  "summary": "Puppet Module to setup opensearch",
  "license": "Apache-2.0",
  "source": "https://github.com/voxpupuli/puppet-opensearch",
  "dependencies": [
    {
      "name": "puppetlabs/apt",
      "version_requirement": ">= 7.0.0 < 11.0.0"
    },
    {
      "name": "puppet/archive",
      "version_requirement": ">= 2.0.0 < 9.0.0"
    },
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">= 1.2.5 < 10.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 9.0.0 < 10.0.0"
    },
    {
      "name": "puppet/systemd",
      "version_requirement": ">= 2.10.0 < 9.0.0"
    },
    {
      "name": "puppet/yum",
      "version_requirement": ">= 5.0.0 < 8.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "10",
        "11",
        "12"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "18.04",
        "20.04",
        "22.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "openvox",
      "version_requirement": ">= 8.19.0 < 9.0.0"
    }
  ]
}
